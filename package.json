{
  "name": "r3shaper-vue",
  "version": "0.0.1",
  "author": "coltor-apps",
  "license": "MIT",
  "description": "R3shaper Vue fetch component",
  "main": "dist/index.js",
  "scripts": {
    "dev": "./node_modules/poi/bin/cli.js",
    "build:demo": "./node_modules/poi/bin/cli.js build",
    "build": "rm -rf ./dist && ./node_modules/bili/dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coltor-apps/r3shaper-vue"
  },
  "keywords": [
    "vue",
    "r3shaper",
    "fetch",
    "component"
  ],
  "bugs": {
    "url": "https://github.com/coltor-apps/r3shaper-vue/issues"
  },
  "peerDependencies": {
    "r3shaper": "^0.1.6",
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "bili": "^3.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "poi": "^10.2.7",
    "rollup-plugin-copied": "^0.0.2",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-vue": "^4.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "dependencies": {
    "p-debounce": "^2.1.0",
    "p-throttle": "^3.1.0"
  }
}